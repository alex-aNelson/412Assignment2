<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    >
    <permission
        android:name="com.example.a412assignment2.MSE412"
        android:protectionLevel="dangerous" />

    <uses-permission android:name="com.example.a412assignment2.MSE412" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-feature android:name="android.hardware.camera" android:required="false" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme._412Assignment2">
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/Theme._412Assignment2">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SecondActivity"
            android:exported="true"
            android:permission="com.example.a412assignment2.MSE412"
            android:label="Second Activity">
            <intent-filter>
                <action android:name="com.example.a412assignment2.OPEN_SECOND_ACTIVITY"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".ViewImageActivity"
            android:exported="false"
            android:label="View Image Activity" />


    </application>

</manifest>